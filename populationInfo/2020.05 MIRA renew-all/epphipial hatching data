library(data.table)
library(ggplot2)

dissected <- fread("/Users/alanbergland/Downloads/DissectedHatchingprop.csv")
dissected[,class:="dissected"]

natural <- fread("/Users/alanbergland/Downloads/EphippiaHatchingprop.csv")
natural[,class:="natural"]
setnames(natural, "Totalhatch", "Total")


m <- rbind(dissected[,c("Clone", "Verndiff", "Total", "prop", "class"), with=F],
           natural[,c("Clone", "Verndiff", "Total", "prop", "class"), with=F])

 ggplot(data=m, aes(x=as.factor(Verndiff), y=prop, color=class)) + geom_point() + facet_wrap(~Clone)



 
